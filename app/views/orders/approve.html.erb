<div id="checkout">
  <h1><%= t("checkout")%></h1>
  <% form_for(@checkout, {
      :url => object_url,
      :html => { :method => :put, :id => 'checkout_form'}
    }) do |checkout_form| %>
    <%= render 'checkouts/payment', :checkout_form => checkout_form %>
    <input id="post-final" type="submit" />
  <% end %>
</div>